<Bindings>	

	<!-- Hooked to the ingamebuttons -->
	<Binding name="GUPPET_AUTO" header="GUPPET">
		GupPet_AutoMounten() ;
	</Binding>
	<Binding name="GUPPET_AUTOGROUND" >
		GupPet_AutoGround() ;
	</Binding>
	<Binding name="GUPPET_AUTOFLY">
		GupPet_AutoFly();
	</Binding>
	<Binding name="GUPPET_CALLCOMPANION">
		GupPet_PreCallCompanion() ;
	</Binding>
	
	<!-- Normal bindings -->
	<Binding name="GUPPET_AQUATICMOUNT" >
		GupPet_PreMounten( "Aquatic" )  ;
	</Binding>
	<Binding name="GUPPET_MULTIGROUNDMOUNT" >
		GupPet_PreMounten( "Multi" )  ;
	</Binding>


	
</Bindings>



